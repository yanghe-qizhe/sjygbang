@{
    ViewBag.Title = "接口日志》详细";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
<script type="text/javascript">
    var KeyValue = GetQuery('KeyValue'); //主键
    $(function () {
        InitControl();
    })
    //得到一个对象实体
    function InitControl() {
        if (!!KeyValue) {
            AjaxJson("/CommonModule/IFLogs/SetFormControl", { KeyValue: KeyValue }, function (data) {
                SetWebControls(data);
            });
        }
    }
</script>
<form id="form1" style="margin: 1px">
<div class="tools_bar" style="margin-top: 1px; margin-bottom: 1px;">
    <a id="lr-replace" class="tools_btn" title="刷新当前(Ctrl+Q)" onclick="Replace()"><span>
        <b style='background: url(@Url.Content("~/Content/Images/Icon16/arrow_refresh.png")) no-repeat 50% 4px;'>
            刷新</b></span></a> <a title="关闭当前窗口(Esc)" onclick="ThisCloseTab()" class="tools_btn">
                <span><b style="background: url(@Url.Content("~/Content/Images/Icon16/back.png")) 50% 4px no-repeat;">
                    离开</b></span></a>
</div>
<div class="bd" style="border-bottom: none; margin: 2px;">
    <div class="tipstitle_title settingtable bold bd todayInfoPanelTab rightPanelTitle_normal">
        <div class="tab_list_top" style="position: absolute">
            <div id="Tabbasic" class="tab_list bd actived" onclick="Tabchange('basic')">
                详细信息</div>
        </div>
    </div>
</div>
<div class="ScrollBar" style="margin: 1px; overflow: hidden;">
    <!--录入信息-->
    <div id="basic" class="tabPanel">
        <table class="form">
            <tr>
                <th class="formTitle">
                    单号：
                </th>
                <td class="formValue">
                    <input id="BILL_NO" maxlength="50" type="text" class="txt" readonly="readonly" />
                </td>
                <th class="formTitle">
                    接口编号：
                </th>
                <td class="formValue">
                    <input id="IF_CODE" type="text" class="txt" readonly="readonly" />
                </td>
                <th class="formTitle">
                    接口名称：
                </th>
                <td class="formValue">
                    <input id="IF_NAME" type="text" class="txt" readonly="readonly" />
                </td>
                <th class="formTitle">
                    接口状态：
                </th>
                <td class="formValue">
                    <input id="IF_IN_SATU" type="text" class="txt" readonly="readonly" />
                </td>
                 <th class="formTitle">
                    接口类型：
                </th>
                <td class="formValue">
                    <input id="IF_TYPE" type="text" class="txt" readonly="readonly" />
                </td>
            </tr>
             <tr>
                <th class="formTitle">
                    发起调用方：
                </th>
                <td class="formValue">
                    <input id="DY_OS" type="text" class="txt" readonly="readonly" />
                </td>
                 <th class="formTitle">
                    接口服务方：
                </th>
                <td class="formValue">
                    <input id="SS_OS" type="text" class="txt" readonly="readonly" />
                </td>
                 <th class="formTitle">
                    调用时间：
                </th>
                <td class="formValue">
                    <input id="BEGIN_TIME" type="text" class="txt" readonly="readonly" />
                </td>
                <th class="formTitle">
                    返回时间：
                </th>
                <td class="formValue">
                    <input id="END_TIME" type="text" class="txt" readonly="readonly" />
                </td>
                <th class="formTitle">
                    执行时长：
                </th>
                <td class="formValue">
                    <input id="IF_RESTIME" type="text" class="txt" readonly="readonly" />
                </td>
            </tr>
            <tr>
                <th class="formTitle">
                    请求数据：
                </th>
                <td class="formValue"  colspan="9">
                    <textarea id="IF_REQUEST" class="txtArea" readonly="readonly"  rows="8"></textarea>
                </td>
              
            </tr>
            <tr>
              <th class="formTitle">
                    返回数据：
                </th>
                <td class="formValue"  colspan="9">
                    <textarea id="IF_RESPOND"  class="txtArea"  readonly="readonly" rows="6" ></textarea>
                </td>
            </tr>
            <tr>
             <th class="formTitle">
                    接口消息：
                </th>
                <td class="formValue"  colspan="9">
                    <textarea id="IF_MSG" class="txtArea" readonly="readonly" rows="6"></textarea>
                </td>
            </tr>
            <tr>
             <th class="formTitle">
                    外部PK值：
                </th>
                <td class="formValue"  colspan="9">
                    <input id="DEF1" type="text" class="txt" readonly="readonly" />
                </td>
            </tr>
        </table>
    </div>
</div>
</form>
