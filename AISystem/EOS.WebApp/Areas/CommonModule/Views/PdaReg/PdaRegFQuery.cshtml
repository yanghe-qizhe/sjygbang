@{
    ViewBag.Title = "手持机》查询";
    Layout = "~/Views/Shared/_LayoutIndex.cshtml";
}
<script>

    $(function () {
       
    })
    //获取条件 
    function GetConditionURL() {
        var url ="";
        var StartTime = $("#StartTime").val();
        var EndTime = $("#EndTime").val();
        var TaskNo = $("#TaskNo").val();
        var Tel = $("#Tel").val();
        var ImeiNo = $("#IMEID").val();
        var Type = $("#DEF2").val();
        url += "&StartTime=" + StartTime + "&EndTime=" + EndTime + "&TaskNo=" + TaskNo;
        url += "&Tel=" + Tel + "&Type=" + Type + "&ImeiNo=" + ImeiNo;
        return url;
    }
    //回调查询确认事件
    function QueryClick(callBack) {
        callBack(GetConditionURL());
        closeDialog();
    }
</script>
<div class="QueryArea">
    <table border="0" class="form-find" style="height: 45px;">
        <tr>
            <th>
                开始时间：
            </th>
            <td>
                <input id="StartTime" type="text" class="txt Wdate" value="2017-01-01" style="width: 200px" onfocus="var endDate=$dp.$('EndTime');WdatePicker({onpicked:function(){EndTime.focus();},maxDate:'#F{$dp.$D(\'EndTime\')}'})" />
            </td>
        </tr>
        <tr>
            <th>
                结束时间：
            </th>
            <td>
                <input id="EndTime" type="text" class="txt Wdate" value="@EOS.Utilities.DateTimeHelper.GetToday()" style="width: 200px" onfocus="WdatePicker({minDate:'#F{$dp.$D(\'StartTime\')}'})" />
            </td>
        </tr>
        <tr>
            <th>
                手持机串号：
            </th>
            <td>
                <input id="IMEID" type="text" class="txt" style="width: 200px" />
            </td>
        </tr>
        <tr>
            <th>
                手持机编号：
            </th>
            <td>
                <input id="TaskNo" type="text" class="txt" style="width: 200px" />
            </td>
        </tr>
        <tr>
            <th>
                手机号码：
            </th>
            <td>
                <input id="Tel" type="text" class="txt" style="width: 200px" />
            </td>
        </tr>   
         <tr>
            <th>
                手持机类型：
            </th>
            <td>
                <select id="DEF2" class="select" style="width: 205px;">
                <option value="">---请选择---</option>
                <option value="1">厂内手持</option>
                <option value="2">客商APP</option>
                </select>
            </td>
        </tr>
    </table>
</div>
