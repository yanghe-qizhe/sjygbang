@{
    ViewBag.Title = "HYBillSOFDetail";
    Layout = "~/Views/Shared/_LayoutIndex.cshtml";
}
<script type="text/javascript">
    var KeyValue = GetQuery('KeyValue'); //主键
    $(function () {
        InitControl();
    })

    //得到一个对象实体
    function InitControl() {
        if (!!KeyValue) {
            AjaxJson("/ExampleModule/DBBillSOF/SetFormControl", { KeyValue: KeyValue }, function (data) {
                SetWebControls(data);
            });
        }
    }
   
</script>
<form id="form1" style="margin: 1px">
<div class="tools_bar" style="margin-top: 1px; margin-bottom: 1px;">
    <a id="lr-replace" class="tools_btn" title="刷新当前(Ctrl+Q)" onclick="Replace()"><span>
        <b style='background: url(@Url.Content("~/Content/Images/Icon16/arrow_refresh.png")) no-repeat 50% 4px;'>
            刷新</b></span></a> <a title="关闭当前窗口(Esc)" onclick="ThisCloseTab()" class="tools_btn">
                <span><b style="background: url(@Url.Content("~/Content/Images/Icon16/back.png")) 50% 4px no-repeat;">
                    离开</b></span></a>
</div>
<table class="form">
    <tr>
        <th class="formTitle">
            发货单位：
        </th>
        <td class="formValue">
            <input id="SEND" type="hidden" dbfield="true" />
            <input id="SENDNAME" maxlength="50" type="text" class="txt" readonly="readonly" />
        </td>
        <th class="formTitle">
            发货仓库：
        </th>
        <td class="formValue">
            <input id="OUTSTACKING" type="hidden" dbfield="true" />
            <input id="OUTSTACKINGNAME" maxlength="50" type="text" class="txt" readonly="readonly" />
        </td>
        <th class="formTitle">
            物料：
        </th>
        <td class="formValue">
            <input id="MATERIAL" type="hidden" />
            <input id="MATERIALNAME" type="text" class="txt" readonly="readonly" />
        </td>
    </tr>
    <tr>
        <th class="formTitle">
            收货单位：
        </th>
        <td class="formValue">
            <input id="RECEIVE" type="hidden" />
            <input id="RECEIVENAME" type="text" class="txt" readonly="readonly" />
        </td>
        <th class="formTitle">
            收货仓库：
        </th>
        <td class="formValue">
            <input id="INSTACKING" type="hidden" />
            <input id="INSTACKINGNAME" type="text" class="txt" readonly="readonly" />
        </td>
        <th class="formTitle">
            规格：
        </th>
        <td class="formValue">
            <input id="MATERIALCODE" type="hidden" />
            <input id="MATERIALSPEC" type="text" class="txt" readonly="readonly" />
        </td>
    </tr>
    <tr>
        <th class="formTitle">
            开始日期：
        </th>
        <td class="formValue">
            <input id="BEGINDATE" type="text" class="txt" readonly="readonly" />
        </td>
        <th class="formTitle">
            结束日期：
        </th>
        <td class="formValue">
            <input id="ENDDATE" type="text" class="txt" readonly="readonly" />
        </td>
        <th class="formTitle">
            制单日期：
        </th>
        <td class="formValue">
            <input id="CREATEDATE" type="text" class="txt" readonly="readonly" />
        </td>
    </tr>
</table>
<div class="bd" style="border-bottom: none; margin: 2px;">
    <div class="tipstitle_title settingtable bold bd todayInfoPanelTab rightPanelTitle_normal">
        <div class="tab_list_top" style="position: absolute">
            <div id="Tabbasic" class="tab_list bd actived" onclick="Tabchange('basic')">
                录入信息</div>
        </div>
    </div>
</div>
<div class="ScrollBar" style="margin: 1px; overflow: hidden;">
    <!--录入信息-->
    <div id="basic" class="tabPanel">
        <table class="form">
            <tr>
                <th class="formTitle">
                    车号：
                </th>
                <td class="formValue">
                    <input id="CARSNAME" type="text" class="txt" readonly="readonly" />
                </td>
                <th class="formTitle">
                    RFID：
                </th>
                <td class="formValue">
                    <input id="RFCARD" type="text" class="txt" readonly="readonly" />
                </td>
            </tr>
            <tr>
                <th class="formTitle">
                    备注：
                </th>
                <td class="formValue" colspan="3">
                    <input id="MEMO" type="text" class="txt" readonly="readonly" />
                </td>
            </tr>
        </table>
    </div>
</div>
</form>
