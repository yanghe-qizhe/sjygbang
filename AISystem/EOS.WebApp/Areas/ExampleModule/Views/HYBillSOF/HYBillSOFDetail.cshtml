@{
    ViewBag.Title = "HYBillSOFDetail";
    Layout = "~/Views/Shared/_LayoutIndex.cshtml";
}
<script type="text/javascript">
    var KeyValue = GetQuery('KeyValue'); //主键
    $(function () {
        InitControl();
    })

    //得到一个对象实体
    function InitControl() {
        if (!!KeyValue) {
            AjaxJson("/ExampleModule/HYBillSOF/SetFormControl", { KeyValue: KeyValue }, function (data) {
                SetWebControls(data);
            });
        }
    }
   
</script>
<form id="form1" style="margin: 1px">
<div class="tools_bar" style="margin-top: 1px; margin-bottom: 1px;">
    <a id="lr-replace" class="tools_btn" title="刷新当前(Ctrl+Q)" onclick="Replace()"><span>
        <b style='background: url(@Url.Content("~/Content/Images/Icon16/arrow_refresh.png")) no-repeat 50% 4px;'>
            刷新</b></span></a> <a title="关闭当前窗口(Esc)" onclick="ThisCloseTab()" class="tools_btn">
                <span><b style="background: url(@Url.Content("~/Content/Images/Icon16/back.png")) 50% 4px no-repeat;">
                    离开</b></span></a>
</div>
<table class="form">
    <tr>
        <th class="formTitle">
            订单编号：
        </th>
        <td class="formValue">
            <input id="VBILLCODE" maxlength="50" type="text" class="txt" readonly="readonly" />
        </td>
        <th class="formTitle">
            发货单号：
        </th>
        <td class="formValue">
            <input id="ID" maxlength="50" type="text" class="txt" readonly="readonly" />
        </td>
        <th class="formTitle">
            业务日期：
        </th>
        <td class="formValue">
            <input id="DBILLDATE" type="text" class="txt" readonly="readonly" />
        </td>
    </tr>
    <tr>
        <th class="formTitle">
            销售组织：
        </th>
        <td class="formValue">
            <input id="ORGNAME" maxlength="50" type="text" class="txt" readonly="readonly" />
        </td>
        <th class="formTitle">
            客户：
        </th>
        <td class="formValue">
            <input id="CUSTNAME" maxlength="50" type="text" class="txt" readonly="readonly" />
        </td>
        <th class="formTitle">
            物料：
        </th>
        <td class="formValue">
            <input id="MATERIALNAME" maxlength="50" type="text" class="txt" readonly="readonly" />
        </td>
    </tr>
    <tr>
        <th class="formTitle">
            订单数量：
        </th>
        <td class="formValue">
            <input id="NASTNUM" maxlength="50" type="text" class="txt" readonly="readonly" />
        </td>
        <th class="formTitle">
            余量：
        </th>
        <td class="formValue">
            <input id="YLNUM" maxlength="50" type="text" class="txt" readonly="readonly" value="0" />
        </td>
        <th class="formTitle">
            部门：
        </th>
        <td class="formValue">
            <input id="DEPTNAME" maxlength="50" type="text" class="txt" readonly="readonly" />
        </td>
    </tr>
</table>
<div class="bd" style="border-bottom: none; margin: 2px;">
    <div class="tipstitle_title settingtable bold bd todayInfoPanelTab rightPanelTitle_normal">
        <div class="tab_list_top" style="position: absolute">
            <div id="Tabbasic" class="tab_list bd actived" onclick="Tabchange('basic')">
                录入信息</div>
        </div>
    </div>
</div>
<div class="ScrollBar" style="margin: 1px; overflow: hidden;">
    <!--录入信息-->
    <div id="basic" class="tabPanel">
        <table class="form">
            <tr>
                <th class="formTitle">
                    车号：
                </th>
                <td class="formValue">
                    <input id="CARSNAME" type="text" class="txt" readonly="readonly" />
                </td>
                <th class="formTitle">
                    车列号：
                </th>
                <td class="formValue">
                    <input id="CARSNO" type="text" class="txt" readonly="readonly" />
                </td>
            </tr>
            <tr>
                <th class="formTitle">
                    发货量：
                </th>
                <td class="formValue">
                    <input id="AMOUNT" type="text" class="txt" readonly="readonly" />
                </td>
                <th class="formTitle">
                    备注：
                </th>
                <td class="formValue">
                    <input id="MEMO" type="text" class="txt" readonly="readonly" />
                </td>
            </tr>
        </table>
    </div>
</div>
</form>
