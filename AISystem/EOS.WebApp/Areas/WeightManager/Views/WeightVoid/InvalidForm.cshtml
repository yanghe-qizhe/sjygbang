@{
    ViewBag.Title = "业务审批》作废一次计量";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
<script type="text/javascript">
    var KeyValue = GetQuery('KeyValue'); //主键
    var ModuleId = GetQuery('ModuleId'); //菜单主键
    $(function () {
        InitControl();
    })

    //得到一个对象实体
    function InitControl() {
        if (!!KeyValue) {
            AjaxJson("/WeightManager/WeightIn/SetFormControl", { KeyValue: KeyValue, ModuleId: ModuleId }, function (data) {
                SetWebControls(data);
            });
        }
    }

    //保存事件
    function AcceptClick() {
        confirmDialog('提示', '注：您确认要作废此操作吗？', function (r) {
            if (r) {
                Loading(true, "正在提交数据...");
                window.setTimeout(function () {
                    var MEMO = $("#MEMO").val();
                    var PK_ORDER = $("#PK_ORDER").val();
                    var parm = 'KeyValue=' + KeyValue + '&PK_ORDER=' + PK_ORDER + "&MEMO=" + escape(MEMO);
                    AjaxJson('/WeightManager/WeightVoid/InvalidOrder', parm, function (data) {
                        tipDialog(data.Message, 3, data.Code);
                        top.frames[tabiframeId()].windowload();
                        closeDialog();
                    });
                }, 200);
            }
        });
    }

    
</script>
<form id="form1" style="margin: 1px">
<div id="message" style="display: none">
</div>
<div class="tools_bar" style="margin-top: 1px; margin-bottom: 1px;">
    <a id="lr-replace" class="tools_btn" title="刷新当前(Ctrl+Q)" onclick="Replace()"><span>
        <b style='background: url(@Url.Content("~/Content/Images/Icon16/arrow_refresh.png")) no-repeat 50% 4px;'>
            刷新</b></span></a>
</div>
<table class="form">
    <tr>
        <th class="formTitle">
            计量单号：
        </th>
        <td class="formValue">
            <input id="BILLNO" maxlength="50" type="text" class="txt readonly" readonly="readonly" />
        </td>
    </tr>
    <tr>
        <th class="formTitle">
            到货单号：
        </th>
        <td class="formValue">
            <input id="PK_ORDER" maxlength="200" type="text" class="txt readonly" readonly="readonly" />
        </td>
    </tr>
    <tr>
        <th class="formTitle">
            车号：
        </th>
        <td class="formValue">
            <input id="CARS" maxlength="200" type="text" class="txt readonly" readonly="readonly" />
        </td>
    </tr>
    <tr>
        <th class="formTitle">
            事由：
        </th>
        <td class="formValue">
            <textarea id="MEMO" name="MEMO" maxlength="200" class="txtArea" rows="4"></textarea>
        </td>
    </tr>
</table>
</form>
