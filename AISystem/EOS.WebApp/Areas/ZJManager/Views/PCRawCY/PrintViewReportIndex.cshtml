@{
    ViewBag.Title = "报表";
    Layout = "~/Views/Shared/_LayoutReportIndex.cshtml";
}
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <script type="text/javascript">
        var ReportName = GetQuery("ReportName");
        var Parameter = GetQuery("Parameter");
        $(function () {
            UpdateToolbar("2");
        })
    </script>
    <style type="text/css">
        html, body
        {
            margin: 0;
            height: 100%;
        }
    </style>
</head>
<body>
    <script type="text/javascript">
        var Parm_Key_Value = GetQuery("Parm_Key_Value");
        var data = GetQuery("Data");
        var height = $(window).height() - 5;
        NewMyCreateReport('@Url.Content("~/Content/GRF/" + Request["Report"])', data, escape(Parm_Key_Value), "100%", height + "px", true);
        SetParameter(Parameter);
    </script>
</body>
</html>
