    function AVGZJRESULT(JsonData) {
        var keys = [];
        $.each(JsonData, function (i) {
            var rowData = JsonData[i];
            keys.push(rowData.ID);
        });
        var KeyValue = keys.join(',');
        var postData = { KeyValue: KeyValue };
        AjaxJson("/ZJManager/PCRawCheck/CheckData", postData, function (data) {
            var JsonData = data.rows;
            var rowindex = 1;
            $.each(JsonData, function (i) {
                var rowData = JsonData[i];
                var index = 1;
                $("#gridTable tbody tr").each(function (i) {
                    var ZJITEM = $("#ZJITEM➩" + index).val();
                    if (ZJITEM == rowData.ZJITEM) {
                        $("#ZJVALUE➩" + index).val(rowData.ZJVALUE);
                    }
                    index++;
                });
                rowindex++;
            });
            CheckData();

        });
    }

    //保存事件
    function SubmitOrderForm() {
        var rows = $("#gridTable").jqGrid("getRowData");
        var len = rows.length;
        if (len == 0) {
            tipDialog("明细信息不能为空!", 3, "warning");
            return false;
        }
        var bol = CheckData();
        if (bol == false) {
            tipDialog("判定不合格，是否复检！", 3, "warning");
            //return false;
        }
        if (issave == true) {
            tipDialog("判定进行中，请稍后保存！", 3, "warning");
            return false;
        }
        if (!CheckDataValid('#form1')) {
            return false;
        }
        confirmDialog('提示', '注：您确认要保存此操作吗？', function (r) {
            if (r) {
                Loading(true, "正在提交数据...");
                window.setTimeout(function () {
                    var postData = GetWebControls("#tb_OrderFrom");
                    postData["OrderEntryJson"] = GetTableDataJson("#gridTable");
                    postData["PCRAWTYPE"] = PCRAWTYPE;
                    AjaxJson("/ZJManager/PCRawCheck/SubmitOrderForm?KeyValue=" + KeyValue + "&ModuleId=" + ModuleId, postData, function (data) {
                        tipDialog(data.Message, 3, data.Code);
                        var tabframeid = "tabs_iframe_" + ModuleId;
                        top.frames[tabframeid].windowload();
                        ThisCloseTab();
                    });
                }, 1000);
            }
        });
    }

    //判定
    function CheckData() {
        var rowindex = 1;
        var checkindex = 0;
        var rows = $("#gridTable").jqGrid("getRowData");
        var len = rows.length;
        for (var index = 0; index < len; index++) {
            var bol = CheckBZPD(rowindex);
            if (bol) {
                checkindex++;
            }
            rowindex++;
        }
        if (checkindex == len) {
            return true;
        } else {
            return false;
        }
    }

    var issave = false;
    function CheckBZPD(index) {
        var checkstatus = 0;
        var checkstatus1 = 0;
        var checkstatus2 = 0;
        var bol = true;
        var zjvaltype = $("#ZJITEMNAME➩" + index).attr("zjvaltype");
        if (zjvaltype == "1") {
            var ZJITEM = $("#ZJITEM➩" + index).val();                                //订单数量
            var ZJFA = $("#ZJFA➩" + index).val();                            //余量
            var ZJVALUE = $("#ZJVALUE➩" + index).val();                //预提
            var ZJITEMNAME = $("#ZJITEMNAME➩" + index).val();
            window.setTimeout(function () {
                var url = "/ZJManager/PCRawCheck/PCRawCHECheckZJBZ?ZJITEMNO=" + escape(ZJITEM) + "&ZJFA=" + escape(ZJFA) + "&ZJValue=" + escape(ZJVALUE) + "&token=" + new Date().getTime();
                getAjax(url, "", function (data) {
                    issave = true;
                    var json = jQuery.parseJSON(data);
                    $("#ZJBZRESULT➩" + index).val(json.Code);
                });
                var rowindex = 1;
                var rows = $("#gridTable").jqGrid("getRowData");
                var len = rows.length;
                for (var i = 0; i < len; i++) {
                    var ZJBZRESULT = $("#ZJBZRESULT➩" + rowindex).val();
                    if (ZJBZRESULT == "1") {
                        checkstatus++;
                    }
                    if (ZJBZRESULT == "2") {
                        checkstatus2++;
                    }
                    rowindex++;
                }
                if (checkstatus2 > 0 && checkstatus == 0) {
                    $("#ZJRESULT").val("2");
                    bol = false;
                }
                else if (checkstatus == 0) {
                    $("#ZJRESULT").val("0");
                }
                else {
                    $("#ZJRESULT").val("1");
                    bol = false;
                }
                issave = false;
            }, 600);
        }

        return bol;
    }

    //强制合格
    function QZCheckData() {
        var rowindex = 1;
        var rows = $("#gridTable").jqGrid("getRowData");
        var len = rows.length;
        if (len == 0) {
            tipDialog("明细信息不能为空!", 3, "");
            return false;
        }
        for (var index = 0; index < len; index++) {
            $("#ZJBZRESULT➩" + rowindex).val("0");
            rowindex++;
        }
        $("#ZJRESULT").val("0");
    }

    //打开读卡器
    //res:1-打开读卡器失败,2-打开射频卡片失败,0-成功。
    var CardNumber = "";
    function CardOpen() {
        var res = 100;
        var version = MWRFATL.OpenReader();
        var iRet = MWRFATL.LastRet;
        //蜂鸣30
        MWRFATL.RF_Beep(30);
        if (iRet != 0) {
            res = 1;
            tipDialog("打开读卡器失败！", 3, "");
            MWRFATL.CloseCard();
            iRet = MWRFATL.LastRet;
            MWRFATL.CloseReader();
            iRet = MWRFATL.LastRet;
        }
        else {
            CardNumber = MWRFATL.OpenCard(1);
            iRet = MWRFATL.LastRet;
            if (iRet != 0) {
                res = 1;
                tipDialog("IC卡没有放在读卡器上！", 3, "");
            }
            else {
                res = 0;
            }
            MWRFATL.CloseReader();
            iRet = MWRFATL.LastRet;
        }
        return res;
    }

    function SetCardNo(CardNumber) {
        var arr = CardNumber.split('');
        var num = "";
        var len = arr.length - 1;
        for (i = len; i >= 0; i = i - 2) {
            num += arr[i - 1] + arr[i];
        }
        var cid = parseInt(num, 16);
        return cid;
    }

    function ReadCard() {
        CardOpen();
        var ICCard = "";

        if (IsNullOrEmpty(CardNumber)) {
            ICCard = SetCardNo(CardNumber);
            $("#DEF6").val(ICCard);
            InitControlICCardZY(ICCard);
        }
        if (ICCard == "") {
            tipDialog("读IC卡失败，IC卡序号为空！", 3, "");
            return false;
        }
    }

    //得到一个制样对象实体
    function InitControlICCardZY(KeyValue) {
