        classid="CLSID:524BFA54-1715-4436-B2B9-DC5E248BC1C3"></object>
<form id="form1" style="margin: 1px">
    <div class="tools_bar" style="margin-top: 1px; margin-bottom: 1px;">
        <a id="lr-replace" class="tools_btn" title="刷新当前(Ctrl+Q)" onclick="Replace()">
            <span>
                <b style='background: url(@Url.Content("~/Content/Images/Icon16/arrow_refresh.png")) no-repeat 50% 4px;'>
                    刷新
                </b>
            </span>
        </a><a id="btn_ReadCard" title="读卡" onclick="ReadCard()" class="tools_btn">
            <span>
                <b style="background: url(@Url.Content("~/Content/Images/Icon16/disk.png")) 50% 4px no-repeat;">
                    读卡
                </b>
            </span>
        </a><a id="button_Commit_Save" title="保存单据" onclick="SubmitOrderForm()"
               class="tools_btn">
            <span>
                <b style="background: url(@Url.Content("~/Content/Images/Icon16/disk.png")) 50% 4px no-repeat;">
                    保存
                </b>
            </span>
        </a><a title="判定" onclick="CheckData()" class="tools_btn">
            <span>
                <b style="background: url(@Url.Content("~/Content/Images/Icon16/ok.png")) 50% 4px no-repeat;">
                    判定
                </b>
            </span>
        </a>@*<a title="强制合格" onclick="QZCheckData()" class="tools_btn"><span><b style="background: url(@Url.Content("~/Content/Images/Icon16/ok.png")) 50% 4px no-repeat;">
                       强制合格</b></span></a><a id="SelZJOrder" title="加权计算" onclick="SelectCheckOrder()"
                           class="tools_btn"> <span><b
            style="background: url(@Url.Content("~/Content/Images/Icon16/application_view_list.png")) 50% 4px no-repeat;">
                               平均结果</b></span></a>*@ <a title="关闭当前窗口(Esc)" onclick="ThisCloseTab()" class="tools_btn">
            <span>
                <b style="background: url(@Url.Content("~/Content/Images/Icon16/back.png")) 50% 4px no-repeat;">
                    离开
                </b>
            </span>
        </a>
    </div>
    <table id="tb_OrderFrom" class="form">
        <tr>
            <th class="formTitle">
                化验单号：
            </th>
            <td class="formValue">
                <input id="BILLNO" type="text" class="txt readonly" readonly="readonly" datacol="yes"
                       err="化验单号" checkexpession="NotNull" value="@ViewBag.BillNo">
            </td>
            <th class="formTitle">
                制单人：
            </th>
            <td class="formValue">
                <input id="CREUSER" type="text" class="txt required readonly" readonly="readonly"
                       err="制单人" checkexpession="NotNull" value="@ViewBag.CreateUserName" />
            </td>
            <th class="formTitle">
                制单日期：
            </th>
            <td class="formValue">
                <input id="CRETIME" type="text" class="txt required readonly" readonly="readonly"
                       err="制单日期" checkexpession="NotNull" value="@DateTime.Now.ToString("yyyy-MM-dd HH:mm:ss")" />
            </td>
        </tr>
        <tr>
            <th class="formTitle">
                制样扫码：
            </th>
            <td class="formValue">
                <input id="ZJFA" type="hidden" />
                <input id="CYID" type="hidden" />
                <input id="PCRAWTYPE" type="hidden" value="0" />
                <input id="PRINTCODE" type="text" class="txt required" datacol="yes" err="制样扫码" checkexpession="NotNull">
            </td>
            <th class="formTitle">
                采样日期：
            </th>
            <td class="formValue">
                <input id="CYDATE" maxlength="50" type="text" class="txt readonly" readonly="readonly"
                       datacol="yes" dbfield="true" err="日期" checkexpession="NotNull" />
            </td>
            <th class="formTitle">
                处置方式：
            </th>
            <td class="formValue">
                <select id="CZTYPE" class="txtselect">
                    <option value="">---请选择---</option>
                    <option value="0" selected="selected">合格</option>
                    <option value="1">让步接收</option>
                    <option value="2">退货</option>
                    <option value="3">合格扣减</option>
                </select>
            </td>
        </tr>
        <tr>
            <th class="formTitle">
                接收工厂：
            </th>
            <td class="formValue">
                <input id="PK_ORG" type="hidden" />
                <input id="ORGNAME" type="text" class="txt" dbfield="true" />
            </td>
            <th class="formTitle">
                物料：
            </th>
            <td class="formValue">
                <input id="MATERIAL" type="hidden" dbfield="true" />
                <input id="MATERIALNAME" type="text" class="txt readonly" readonly="readonly" />
            </td>
            <th class="formTitle">
                规格：
            </th>
            <td class="formValue">
                <input id="MATERIALSPEC" type="text" class="txt readonly" readonly="readonly" />
            </td>
        </tr>
        <tr>
            <th class="formTitle">
                车号：
            </th>
            <td class="formValue">
                <input id="CARS" type="hidden" dbfield="true" />
                <input id="CARSNAME" type="text" class="txt readonly" readonly="readonly" />
            </td>
            <th class="formTitle">
                IC卡序号：
            </th>
            <td class="formValue">
                <input id="DEF6" type="text" class="txt readonly" readonly="readonly" />
            </td>
            <th class="formTitle">
                化验人：
            </th>
            <td class="formValue">
                <input id="DEF10" type="text" class="txt " datacol="yes" dbfield="true" err="化验人"
                       checkexpession="NotNull" value="@EOS.Utilities.ManageProvider.Provider.Current().UserName" />
            </td>
        </tr>
        <tr>
            <th class="formTitle">
                合格判定：
            </th>
            <td class="formValue">
                <select id="ZJRESULT" class="txtselect" datacol="yes" err="合格判定" checkexpession="NotNull"
                        disabled="disabled">
                    <option value="">---请选择---</option>
                    <option value="0">检验合格</option>
                    <option value="1">检验不合格</option>
                    <option value="2" selected="selected">未检验</option>
                </select>
            </td>
            <th class="formTitle">
                备注：
            </th>
            <td class="formValue" colspan="3">
                <input id="MEMO" type="text" class="txt" />
            </td>
        </tr>
    </table>
    <div class="bd" style="border-bottom: none; margin: 2px;">
        <div class="tipstitle_title settingtable bold bd todayInfoPanelTab rightPanelTitle_normal">
            <div class="tab_list_top" style="position: absolute">
                <div id="Taborder" class="tab_list bd actived" onclick="Tabchange('order')">
                    质检信息
                </div>
            </div>
        </div>
    </div>
    <div class="ScrollBar" style="margin: 1px; overflow: hidden;">
        <!--拼单信息-->
        <div id="order" class="tabPanel">
            <table id="gridTable"></table>
        </div>
    </div>
</form>
