    function Conversion(rowindex) {
        //统计合计
        var TotalCARSNUM = 0, TotalYFSUTTLE = 0.0, TotalGROSS = 0.0, TotalTARE = 0.0, TotalSUTTLE = 0.0;
        var TotalYFPIECE = 0.0, TotalJJSUTTLE = 0.0, TotalZJSUTTLE = 0.0, TotalTZSUTTLE = 0.0, TotalBDSUTTLE = 0.0;
        var TotalJSSUTTLE = 0.0, TotalCHARGE = 0.0, TotalPDAKZ2 = 0.0;
        $("#gridTable tbody tr").each(function (i) {
            var BILLNO = $(this).find('td:eq(2)').find('input').val();
            if (BILLNO != "" && BILLNO != undefined) {
                TotalCARSNUM = TotalCARSNUM + 1;
            }
            var YFSUTTLE = $(this).find('td:eq(9)').find('input').val();
            if (YFSUTTLE != "" && YFSUTTLE != undefined) {
                TotalYFSUTTLE = FloatAdd(TotalYFSUTTLE, YFSUTTLE);
            }
            var GROSS = $(this).find('td:eq(10)').find('input').val();
            if (GROSS != "" && GROSS != undefined) {
                TotalGROSS = FloatAdd(TotalGROSS, GROSS);
            }
            var TARE = $(this).find('td:eq(11)').find('input').val();
            if (TARE != "" && TARE != undefined) {
                TotalTARE = FloatAdd(TotalTARE, TARE);
            }
            var SUTTLE = $(this).find('td:eq(12)').find('input').val();
            if (SUTTLE != "" && SUTTLE != undefined) {
                TotalSUTTLE = FloatAdd(TotalSUTTLE, SUTTLE);
            }
            var YFPIECE = $(this).find('td:eq(13)').find('input').val();
            if (YFPIECE != "" && YFPIECE != undefined) {
                TotalYFPIECE = FloatAdd(TotalYFPIECE, YFPIECE);
            }
            var PDAKZ2 = $(this).find('td:eq(14)').find('input').val();
            if (PDAKZ2 != "" && PDAKZ2 != undefined) {
                TotalPDAKZ2 = FloatAdd(TotalPDAKZ2, PDAKZ2);
            }

            var JJSUTTLE = $(this).find('td:eq(15)').find('input').val();
            if (JJSUTTLE != "" && JJSUTTLE != undefined) {
                TotalJJSUTTLE = FloatAdd(TotalJJSUTTLE, JJSUTTLE);
            }
            var ZJSUTTLE = $(this).find('td:eq(16)').find('input').val();
            if (ZJSUTTLE != "" && ZJSUTTLE != undefined) {
                TotalZJSUTTLE = FloatAdd(TotalZJSUTTLE, ZJSUTTLE);
            }
            var TZSUTTLE = $(this).find('td:eq(17)').find('input').val();
            if (TZSUTTLE != "" && TZSUTTLE != undefined) {
                TotalTZSUTTLE = FloatAdd(TotalTZSUTTLE, TZSUTTLE);
            }
            var BDSUTTLE = $(this).find('td:eq(18)').find('input').val();
            if (BDSUTTLE != "" && BDSUTTLE != undefined) {
                TotalBDSUTTLE = FloatAdd(TotalBDSUTTLE, BDSUTTLE);
            }
            var JSSUTTLE = $(this).find('td:eq(19)').find('input').val();
            if (JSSUTTLE != "" && JSSUTTLE != undefined) {
                TotalJSSUTTLE = FloatAdd(TotalJSSUTTLE, JSSUTTLE);
            }
            var CHARGE = $(this).find('td:eq(21)').find('input').val();
            if (CHARGE != "" && CHARGE != undefined) {
                TotalCHARGE = FloatAdd(TotalCHARGE, CHARGE);
            }
        });
        //车数
        $("#TotalCARSNUM").text(TotalCARSNUM);
        $("#CARSNUM").val(TotalCARSNUM);
        //应收重量
        $("#TotalYFSUTTLE").text(getFloat(TotalYFSUTTLE, 3));
        $("#YFSUTTLE").val(getFloat(TotalYFSUTTLE, 3));
        //毛重
        $("#TotalGROSS").text(getFloat(TotalGROSS, 3));
        //皮重
        $("#TotalTARE").text(getFloat(TotalTARE, 3));
        //净重
        $("#TotalSUTTLE").text(getFloat(TotalSUTTLE, 3));
        $("#SUTTLE").val(getFloat(TotalSUTTLE, 3));
        //件数
        $("#TotalYFPIECE").text(getFloat(TotalYFPIECE, 3));
        $("#YFPIECE").val(getFloat(TotalYFPIECE, 0));
        //充公重量 
        $("#TotalPDAKZ2").text(getFloat(TotalPDAKZ2, 3));
        $("#PDAKZ2").val(getFloat(TotalPDAKZ2, 3));

        //采购部检斤重量
        $("#TotalJJSUTTLE").text(getFloat(TotalJJSUTTLE, 3));
        $("#JJSUTTLE").val(getFloat(TotalJJSUTTLE, 3));
        //采购部检斤后重量
        var JJSUTTLE1 = 0;
        if (TotalJJSUTTLE > 0) {
            JJSUTTLE1 = FloatSub(TotalSUTTLE, TotalJJSUTTLE);
            $("#JJSUTTLE1").val(getFloat(JJSUTTLE1, 3));
        }
        //质检扣重
        $("#TotalZJSUTTLE").text(getFloat(TotalZJSUTTLE, 3));
        $("#ZJSUTTLE").val(getFloat(TotalZJSUTTLE, 3));
        //扣后重量
        var ZJSUTTLE1 = 0;
        if (TotalZJSUTTLE > 0) {
            ZJSUTTLE1 = FloatSub(TotalSUTTLE, TotalZJSUTTLE);
            $("#ZJSUTTLE1").val(getFloat(ZJSUTTLE1, 3));
        }
        //调整重理
        $("#TotalTZSUTTLE").text(getFloat(TotalTZSUTTLE, 3));
        $("#TZSUTTLE").val(getFloat(TotalTZSUTTLE, 3));
        //水分补点
        $("#TotalBDSUTTLE").text(getFloat(TotalBDSUTTLE, 3));
        $("#BDSUTTLE").val(getFloat(TotalBDSUTTLE, 3));
        //结算重量
        $("#TotalJSSUTTLE").text(getFloat(TotalJSSUTTLE, 3));
        $("#JSSUTTLE").val(getFloat(TotalJSSUTTLE, 3));
        //金额 
        $("#TotalCHARGE").text(getFloat(TotalCHARGE, 2));
        $("#CHARGE").val(getFloat(TotalCHARGE, 2));
        var CHINESECHARGE = ChargeChinese(TotalCHARGE);
        $("#CHINESECHARGE").val(CHINESECHARGE);

    }

