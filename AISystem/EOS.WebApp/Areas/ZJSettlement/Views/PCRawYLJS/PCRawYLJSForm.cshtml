    function GetBillCode() {
        var strDate = $("#JSDATE").val();
        var strCode = $("#WEIGHTNO").val();
        if (strCode != "") {
            AjaxJson("/ZJSettlement/PCRawYLJS/GetBillCode", { strCode: strCode, strDate: strDate, ModuleId: ModuleId, type: "0" }, function (data) {
                $("#BILLNO").val(data.billno);
            });
        }
    }

    function WEIGHTNOAutocomplete() {
        var txtname = "WEIGHTNAME";
        var $txtid = $("#WEIGHTNO");
        var $txtname = $("#WEIGHTNAME");

        var width = "";
        if (width == "") {
            width = $txtname.width();
        }
        var height = "";
        if (height == "") {
            height = "150";
        }
        $txtname.bind("keyup", function (e) {
            if (e.which != 13 && e.which != 40 && e.which != 38) {
                var keyvalue = $(this).val();
                if (keyvalue.length >= 3) {
                    var keywords = $txtname.val()
                    DataSource(keywords);
                }
                $txtid.val("");
            }
        }).focus(function () {
            $(this).select();
            DataSource("");
        });
        //上，下键盘回调
        autocompletekeydown(txtname, function (data) {
            $txtid.val(data.id);
            $txtname.val(data.name);
            GetBillCode();
        });
        //获取数据源
        function DataSource(keywords) {
            AjaxJson("/AutoData/WeightConfig", { keywords: keywords }, function (DataJson) {
                var html = "";
                $.each(DataJson, function (i) {
                    html += "<tr>";
                    html += '<td id="id" style="display: none;">' + DataJson[i].id + '</td>';
                    html += '<td id="name" style="width: 100%;">' + DataJson[i].name + '</td>';
                    html += "</tr>";
                });
                //点击事件回调
                autocomplete(txtname, width + "px", height + "px", html, function (data) {
                    $txtid.val(data.id);
                    $txtname.val(data.name);
                    GetBillCode();
                });
            });
        }
    }

    //得到一个对象实体
    function InitControl() {
        if (IsNull(KeyValue)) {
            AjaxJson("/ZJSettlement/PCRawYLJS/SetFormControl", { KeyValue: KeyValue }, function (data) {
                SetWebControls(data);
            });
        }
    }

    function InitJYControl() {
        AjaxJson("/ZJSettlement/PCRawYLJS/GetJYOrderEntity", null, function (data) {
            if (data.WEIGHTNO != null) {
                $("#BEGINDATE").val(data.BEGINDATE);
                $("#ENDDATE").val(data.ENDDATE);
                $("#WEIGHTNO").val(data.WEIGHTNO);
                $("#WEIGHTNAME").val(data.WEIGHTNAME);
                $("#JSDATE").val(data.JSDATE);
                $("#PK_RECEIVEORG").val(data.PK_RECEIVEORG);
                $("#RECEIVEORG").val(data.RECEIVEORG);
                GetBillCode();
            }
        });
    }

